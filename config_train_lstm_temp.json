{
    "input_csv":"data/meteorologico/A620_temp.csv",
    "output_log_file":"train_lstm_temp.log",
    "output_models_path":"models/",
    "output_results_path":"results/",
    "n_past_steps":3,
    "date_col":"date",
    "hr_col":"hr",
    "numeric_var":"temp",
    "sensor_var":"inme",
    "target_sensor":"A620",
    "early_stop_patience":10,
    "epochs":3,
    "base_config":{
    	"first_layer":{"mult":2,"dropout_rate":0.2,"dropout_range":[0,1]},
    	"last_layer":{"mult":2,"dropout_rate":0.2,"dropout_range":[0,1]}
    },
    "mid_layers_config":{"n_layers":2,"mult":2,"dropout_rate":0.2,"dropout_range":[0,1]},
    "hyperopt_pars":{"mid_layers":[2,3],"mults":[2,3],"dropout_rate_range":[0.1,0.8],"max_evals":2},
    "model_loss":"mean_squared_error",
	"optimizer":"adam"
}
